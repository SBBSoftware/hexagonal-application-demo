<!DOCTYPE html>
<html>
<head>
  <title>SbbHexagonalDemos</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="robots" content="noindex, nofollow"/>
  <meta name="description" content="Test bed for demos"/>
  <meta name="keywords" content="rails ruby activemodel"/>
</head>
<body role="document">
<!-- Navigation -->
<%= render 'shared/top_nav' %>

<!-- page title -->

<!-- content section a -->
<div class="content-section-a">


  <div class="clearfix"></div>
  <div class="container"><h2 class="section-heading"><%= content_for(:titles) -%></h2></div>
  <!-- errors block -->
  <div class="container">
    <% if notice %>
      <div class="row">
        <div>
          <div class="alert alert-success alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            <%= notice %>
          </div>
        </div>
      </div>
    <% end %>
    <% if alert %>
      <div class="row">
        <div>
          <div class="alert alert-danger alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            <%= alert %>
          </div>
        </div>
      </div>
    <% end %>
  </div>


  <div class="container">
    <%= yield %>
  </div>
  <!-- /.container -->

</div>
<!-- end content section a -->





<!-- Footer -->
<%= render 'shared/footer' %>

</body>
</html>
